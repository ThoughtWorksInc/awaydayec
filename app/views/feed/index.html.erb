<a href="#hashtag-anchor" id="up" class="hashtag-title">
<div id="logo">
    <%= image_tag("tw_logo.png", height: "52", alt: "Edit Entry") %>
    <span class="hashtag">#<%=ENV["HASHTAG"]%></span>
  </div>
</a>
<a id="hashtag-anchor"></a>

<div class="flexslider">
  <ul class="slides">
    <% @posts.each_with_index do |post, i| %>
      <% if post.media_url %>
        <%= render 'post', post: post, i: i %>
      <% end %>
    <% end %>
  </ul>
</div>

<h1 id="loading"></h1>